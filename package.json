{
  "name": "austincodingacademy.com",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "echo \"Error: no test specified\"",
    "js": "mkdir -p assets/javascripts && browserify -g uglifyify _javascripts/app.js --debug | exorcist assets/javascripts/app.js.map > assets/javascripts/app.js",
    "images": "imagemin _images/* --out-dir=assets/images",
    "sass": "npm run sass-austin && npm run sass-sanantonio && npm run sass-dallas && npm run sass-houston && npm run sass-include",
    "postinstall": "npm run sass && npm run js && npm run images",
    "sass-austin": "node-sass --output-style compressed -o assets/stylesheets/ _sass/austin.scss",
    "sass-sanantonio": "node-sass --output-style compressed -o assets/stylesheets/ _sass/sanantonio.scss",
    "sass-dallas": "node-sass --output-style compressed -o assets/stylesheets/ _sass/dallas.scss",
    "sass-houston": "node-sass --output-style compressed -o assets/stylesheets/ _sass/houston.scss",
    "sass-include": "mkdir _includes/stylesheets && cp -R assets/stylesheets/. _includes/stylesheets",
    "build-austin": "bundle exec jekyll build --config _config_austin.yml",
    "build-dallas": "bundle exec jekyll build --config _config_dallas.yml",
    "build-houston": "bundle exec jekyll build --config _config_houston.yml",
    "build-sanantonio": "bundle exec jekyll build --config _config_sanantonio.yml",
    "serve-austin": "bundle exec jekyll serve --config _config_austin.yml",
    "serve-houston": "bundle exec jekyll serve --config _config_houston.yml",
    "serve-sanantonio": "bundle exec jekyll serve --config _config_sanantonio.yml",
    "serve-dallas": "bundle exec jekyll serve --config _config_dallas.yml",
    "sitemap-generator": "(http-server _site &) && (sitemap-generator http://127.0.0.1:8080 > _site/sitemap.xml)",
    "sitemap-austin": "npm run sitemap-generator && sed -i 's|http://127.0.0.1:8080|https://austincodingacademy.com|g' _site/sitemap.xml",
    "sitemap-sanantonio": "npm run sitemap-generator && sed -i 's|http://127.0.0.1:8080|https://sanantoniocodingacademy.com|g' _site/sitemap.xml",
    "sitemap-houston": "npm run sitemap-generator && sed -i 's|http://127.0.0.1:8080|https://houstontxcodingacademy.com|g' _site/sitemap.xml",
    "sitemap-dallas": "npm run sitemap-generator && sed -i 's|http://127.0.0.1:8080|https://dallascodingacademy.com|g' _site/sitemap.xml",
    "favicon-inject": "echo '' > _includes/favicon.html && real-favicon inject faviconData.json assets/favicons _includes/favicon.html && mv assets/favicons/favicon.html _includes/favicon.html",
    "favicon-austin": "real-favicon generate _favicon_austin.json faviconData.json assets/favicons/ && npm run favicon-inject",
    "favicon-sanantonio": "real-favicon generate _favicon_sanantonio.json faviconData.json assets/favicons/ && npm run favicon-inject",
    "favicon-dallas": "real-favicon generate _favicon_dallas.json faviconData.json assets/favicons/ && npm run favicon-inject",
    "favicon-houston": "real-favicon generate _favicon_houston.json faviconData.json assets/favicons/ && npm run favicon-inject",
    "deploy-austin": "echo 'austincodingacademy.com' >> _site/CNAME && gh-pages --repo 'git@github.com:AustinCodingAcademy/austincodingacademy.com.git' -d ./_site",
    "deploy-houston": "echo 'houstontxcodingacademy.com' >> _site/CNAME && gh-pages --repo 'git@github.com:AustinCodingAcademy/houstontxcodingacademy.com.git' -d ./_site",
    "deploy-sanantonio": "echo 'sanantoniocodingacademy.com' >> _site/CNAME && gh-pages --repo 'git@github.com:AustinCodingAcademy/sanantoniocodingacademy.com.git' -d ./_site",
    "deploy-dallas": "echo 'dallascodingacademy.com' >> _site/CNAME && gh-pages --repo 'git@github.com:AustinCodingAcademy/dallascodingacademy.com.git' -d ./_site",
    "deploy-austin-preview": "echo 'preview.austincodingacademy.com' >> _site/CNAME && gh-pages --repo 'git@github.com:AustinCodingAcademy/preview.austincodingacademy.com.git' -d ./_site",
    "deploy-houston-preview": "echo 'preview.houstontxcodingacademy.com' >> _site/CNAME && gh-pages --repo 'git@github.com:AustinCodingAcademy/preview.houstontxcodingacademy.com.git' -d ./_site",
    "deploy-sanantonio-preview": "echo 'preview.sanantoniocodingacademy.com' >> _site/CNAME && gh-pages --repo 'git@github.com:AustinCodingAcademy/preview.sanantoniocodingacademy.com.git' -d ./_site",
    "deploy-dallas-preview": "echo 'preview.dallascodingacademy.com' >> _site/CNAME && gh-pages --repo 'git@github.com:AustinCodingAcademy/preview.dallascodingacademy.com.git' -d ./_site",
    "no-robots": "touch robots.txt && echo 'User-agent: *' > robots.txt && echo 'Disallow: /' >> robots.txt",
    "watch": "npm-watch"
  },
  "watch": {
    "sass": {
      "patterns": [
        "_sass"
      ],
      "extensions": "scss",
      "inherit": true
    },
    "js": {
      "patterns": [
        "_javascripts"
      ],
      "extensions": "js",
      "inherit": true
    },
    "images": {
      "patterns": [
        "_images"
      ],
      "extensions": "jpg,jpeg,png,tiff,gif,svg",
      "inherit": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AustinCodingAcademy/austincodingacademy.com.git"
  },
  "author": "Kevin Colten",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AustinCodingAcademy/austincodingacademy.com/issues"
  },
  "homepage": "https://github.com/AustinCodingAcademy/austincodingacademy.com#readme",
  "dependencies": {
    "bootstrap-block-grid": "^1.1.7",
    "bootstrap-sass": "^3.3.7",
    "browserify": "^13.3.0",
    "exorcist": "^0.4.0",
    "font-awesome": "^4.7.0",
    "imagemin-cli": "^3.0.0",
    "javascript-detect-element-resize": "^0.5.3",
    "jquery": "^3.1.1",
    "jquery-match-height": "^0.7.2",
    "moment": "^2.17.1",
    "node-sass": "^4.3.0",
    "pygments-rouge-css": "^0.1.0",
    "slick-carousel": "^1.6.0"
  },
  "browser": {
    "bootstrap.affix": "./node_modules/bootstrap-sass/assets/javascripts/bootstrap/affix.js",
    "bootstrap.alert": "./node_modules/bootstrap-sass/assets/javascripts/bootstrap/alert.js",
    "bootstrap.button": "./node_modules/bootstrap-sass/assets/javascripts/bootstrap/button.js",
    "bootstrap.carousel": "./node_modules/bootstrap-sass/assets/javascripts/bootstrap/carousel.js",
    "bootstrap.collapse": "./node_modules/bootstrap-sass/assets/javascripts/bootstrap/collapse.js",
    "bootstrap.dropdown": "./node_modules/bootstrap-sass/assets/javascripts/bootstrap/dropdown.js",
    "bootstrap.modal": "./node_modules/bootstrap-sass/assets/javascripts/bootstrap/modal.js",
    "bootstrap.popover": "./node_modules/bootstrap-sass/assets/javascripts/bootstrap/popover.js",
    "bootstrap.scrollspy": "./node_modules/bootstrap-sass/assets/javascripts/bootstrap/scrollspy.js",
    "bootstrap.tab": "./node_modules/bootstrap-sass/assets/javascripts/bootstrap/tab.js",
    "bootstrap.tooltip": "./node_modules/bootstrap-sass/assets/javascripts/bootstrap/tooltip.js",
    "bootstrap.transition": "./node_modules/bootstrap-sass/assets/javascripts/bootstrap/transition.js",
    "jquery.resize.js": "./node_modules/javascript-detect-element-resize/jquery.resize.js"
  },
  "devDependencies": {
    "cli-real-favicon": "0.0.6",
    "gh-pages": "^0.12.0",
    "http-server": "^0.9.0",
    "npm-watch": "^0.1.8",
    "sitemap-generator-cli": "^5.0.1"
  }
}
